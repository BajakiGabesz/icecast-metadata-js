{
  "name": "demo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "cra-append-sw": "^2.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.2",
    "serve": "^11.3.2"
  },
  "scripts": {
    "start": "react-scripts start && cra-append-sw --mode dev ./src/icecast-sw.js",
